{
  "name": "dusty-fns",
  "version": "0.5.0",
  "description": "A small library of functional operators inspired by Ramda",
  "main": "./dusty.min.js",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.40",
    "@babel/preset-env": "^7.0.0-beta.40",
    "ava": "0.25.0",
    "babel-loader": "^8.0.0-beta.0",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "del": "3.0.0",
    "eslint": "4.17.0",
    "globby": "7.1.1",
    "jsdoc-to-markdown": "4.0.1",
    "uglifyjs-webpack-plugin": "1.1.8",
    "webpack": "3.10.0"
  },
  "scripts": {
    "prepack": "webpack && node scripts/create-export.js && webpack --config webpack.dist.js && node scripts/docs.js",
    "postpack": "npm run clean",
    "build": "webpack --display-modules",
    "clean": "node scripts/clean.js",
    "postpublish": "npm run clean",
    "test:watch": "ava --watch",
    "test": "ava --verbose"
  },
  "ava": {
    "files": [
      "src/**/tests.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "concurrency": 5,
    "require": [
      "babel-register"
    ],
    "babel": {
      "presets": [
        "env",
        "@ava/stage-4",
        "@ava/transform-test-files"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhershman1/dustyjs.git"
  },
  "keywords": [
    "dusty",
    "functional",
    "library",
    "operators",
    "helpers",
    "utils",
    "utilities",
    "utility"
  ],
  "author": "Dustin Hershman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dhershman1/dustyjs/issues"
  },
  "homepage": "https://github.com/dhershman1/dustyjs#readme"
}
