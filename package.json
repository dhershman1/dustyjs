{
  "name": "dusty-fns",
  "version": "0.1.0",
  "description": "A small library of functional operators inspired by Ramda",
  "main": "dist/dusty-fns.umd.js",
  "devDependencies": {
    "eslint": "4.4.1",
    "rollup": "0.49.3",
    "rollup-plugin-buble": "0.15.0",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-uglify": "2.0.1"
  },
  "scripts": {
    "rollup": "rollup -c",
    "test:watch": "ava --watch",
    "test": "ava"
  },
  "babel": {
    "presets": [
      "env",
      "@ava/stage-4",
      "@ava/transform-test-files"
    ]
  },
  "ava": {
    "files": [
      "src/**/tests.js",
      "src/tests/*.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "concurrency": 5,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhershman1/dustyjs.git"
  },
  "keywords": [
    "dusty",
    "functional",
    "library",
    "operators",
    "helpers",
    "utils",
    "utilities",
    "utility"
  ],
  "author": "Dustin Hershman",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/dhershman1/dustyjs/issues"
  },
  "homepage": "https://github.com/dhershman1/dustyjs#readme",
  "dependencies": {
    "ava": "^0.24.0",
    "babel": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0"
  }
}
