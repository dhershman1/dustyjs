{
  "name": "dusty-fns",
  "version": "2.2.0",
  "description": "A small library of functional operators inspired by Ramda",
  "main": "dist/dusty.min.js",
  "devDependencies": {
    "@babel/core": "7.0.0-beta.44",
    "@babel/preset-env": "7.0.0-beta.44",
    "@babel/preset-stage-0": "7.0.0-beta.44",
    "@babel/preset-stage-2": "7.0.0-beta.44",
    "@babel/register": "7.0.0-beta.44",
    "ava": "1.0.0-beta.4",
    "del": "3.0.0",
    "globby": "8.0.1",
    "jsdoc-to-markdown": "4.0.1",
    "rollup": "0.58.1",
    "rollup-plugin-babel": "4.0.0-beta.4",
    "rollup-plugin-cleanup": "2.0.1",
    "rollup-plugin-filesize": "1.5.0",
    "rollup-plugin-uglify": "3.0.0",
    "standard": "11.0.1"
  },
  "scripts": {
    "prepack": "rollup -c rollup.split.js && node scripts/create-export.js && rollup -c && node scripts/docs.js",
    "postpack": "npm run clean",
    "build": "rollup -c",
    "split": "rollup -c rollup.split.js",
    "scripts": "node scripts/create-export.js && node scripts/docs.js",
    "stats": "rollup -c rollup.split.js && node scripts/clean.js",
    "clean": "node scripts/clean.js",
    "postpublish": "npm run clean",
    "test:watch": "ava --watch",
    "test": "ava",
    "lint": "standard src/**/*.js"
  },
  "ava": {
    "files": [
      "test/*.js"
    ],
    "require": [
      "@babel/register"
    ],
    "failFast": true,
    "concurrency": 5,
    "babel": {
      "testOptions": {
        "presets": [
          "@ava/transform-test-files",
          "@ava/stage-4"
        ]
      }
    }
  },
  "standard": {
    "ignore": [
      "dist/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhershman1/dustyjs.git"
  },
  "keywords": [
    "dusty",
    "functional",
    "library",
    "pure",
    "operators",
    "helpers",
    "utils",
    "utilities",
    "utility",
    "single type"
  ],
  "author": "Dustin Hershman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dhershman1/dustyjs/issues"
  },
  "homepage": "https://github.com/dhershman1/dustyjs#readme"
}
