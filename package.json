{
  "name": "dusty-fns",
  "version": "2.0.1",
  "description": "A small library of functional operators inspired by Ramda",
  "main": "./dusty.min.js",
  "devDependencies": {
    "@babel/core": "7.0.0-beta.42",
    "@babel/plugin-external-helpers": "7.0.0-beta.42",
    "@babel/preset-env": "7.0.0-beta.42",
    "@babel/preset-stage-0": "7.0.0-beta.42",
    "@babel/preset-stage-2": "7.0.0-beta.42",
    "@babel/register": "7.0.0-beta.42",
    "ava": "1.0.0-beta.3",
    "del": "3.0.0",
    "eslint": "4.18.0",
    "globby": "8.0.1",
    "jsdoc-to-markdown": "4.0.1",
    "rollup": "0.56.5",
    "rollup-plugin-babel": "4.0.0-beta.2",
    "rollup-plugin-uglify": "3.0.0"
  },
  "scripts": {
    "prepack": "rollup -c rollup.split.js && node scripts/create-export.js && rollup -c && node scripts/docs.js",
    "postpack": "npm run clean",
    "build": "rollup -c",
    "split": "rollup -c rollup.split.js",
    "clean": "node scripts/clean.js",
    "postpublish": "npm run clean",
    "test:watch": "ava --watch",
    "test": "ava"
  },
  "ava": {
    "files": [
      "src/**/tests.js"
    ],
    "require": [
      "@babel/register"
    ],
    "failFast": true,
    "concurrency": 5,
    "babel": {
      "testOptions": {
        "presets": [
          "@ava/transform-test-files",
          "@ava/stage-4"
        ]
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhershman1/dustyjs.git"
  },
  "keywords": [
    "dusty",
    "functional",
    "library",
    "operators",
    "helpers",
    "utils",
    "utilities",
    "utility"
  ],
  "author": "Dustin Hershman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dhershman1/dustyjs/issues"
  },
  "homepage": "https://github.com/dhershman1/dustyjs#readme"
}
