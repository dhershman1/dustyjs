{
  "name": "dusty-fns",
  "version": "0.2.2",
  "description": "A small library of functional operators inspired by Ramda",
  "main": "./index.js",
  "devDependencies": {
    "ava": "0.24.0",
    "babel": "6.23.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "1.6.1",
    "babel-register": "6.26.0",
    "buble-loader": "0.4.1",
    "del": "3.0.0",
    "eslint": "4.14.0",
    "globby": "7.1.1",
    "webpack": "3.10.0"
  },
  "scripts": {
    "prepare": "node scripts/create-export.js && webpack",
    "clean": "node scripts/clean.js",
    "postpublish": "npm run clean",
    "test:watch": "ava --watch",
    "test": "ava"
  },
  "babel": {
    "presets": [
      "env",
      "@ava/stage-4",
      "@ava/transform-test-files"
    ]
  },
  "ava": {
    "files": [
      "src/**/tests.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "concurrency": 5,
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhershman1/dustyjs.git"
  },
  "keywords": [
    "dusty",
    "functional",
    "library",
    "operators",
    "helpers",
    "utils",
    "utilities",
    "utility"
  ],
  "author": "Dustin Hershman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dhershman1/dustyjs/issues"
  },
  "homepage": "https://github.com/dhershman1/dustyjs#readme"
}
